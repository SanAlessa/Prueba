urn (\r\n    <>\r\n    {/* LLAMA AL COMPARADOR DE RUTA */}\r\n    <PathComparator/>\r\n    <div id=\"header\" className=\"principalHeader\">\r\n      <Navbar color=\"faded\" light expand=\"md\">\r\n        <NavbarToggler onClick={toggle} />\r\n          <Collapse isOpen={isOpen} navbar>\r\n            <Nav className=\"mr-auto\" navbar>\r\n              <NavItem>\r\n                <NavLink className=\"active\" to=\"/\">Home</NavLink>\r\n              </NavItem>\r\n              <NavItem>\r\n                <NavLink className=\"active\" to=\"/cities\">Cities</NavLink>\r\n              </NavItem>\r\n              {links}\r\n            </Nav>\r\n            </Collapse>\r\n        </Navbar>\r\n        <div>\r\n          <img id=\"logo\" src=\"../assets/logo.png\" alt=\"logo\" style={{visibility: 'hidden', width: '4vw', marginLeft: '-5vw', minWidth: '50px'}}/>\r\n        </div>\r\n        <div style={{display: 'flex', alignItems: 'center'}}>\r\n          {props.userLogged && <h2 style={{color: 'white'}}>Hi! Welcome, {props.userLogged.response.name}</h2>}\r\n          <div className=\"imgLogIn\" id=\"imgLogin\" style={{backgroundImage: `url(${props.userLogged ? props.userLogged.response.pic : '../assets/login1.png'})`}} alt=\"Login img\" />\r\n        </div>\r\n    </div>\r\n    </>\r\n  )\r\n}\r\n\r\nconst mapStateToProps = state => {\r\n  return {\r\n    userLogged: state.userR.userLogged\r\n  }\r\n}\r\n\r\nconst mapDispatchToProps = {\r\n  logOut: userActions.logOut\r\n}\r\n\r\n\r\nexport default connect(mapStateToProps, mapDispatchToProps)(Header)","D:\\Users\\Santiago\\Documents\\MindHub\\alessandro-mytinerary\\frontend\\src\\components\\Logo.js",[],"D:\\Users\\Santiago\\Documents\\MindHub\\alessandro-mytinerary\\frontend\\src\\components\\Mytinerary.js",[],"D:\\Users\\Santiago\\Documents\\MindHub\\alessandro-mytinerary\\frontend\\src\\components\\Carousel.js",[],"D:\\Users\\Santiago\\Documents\\MindHub\\alessandro-mytinerary\\frontend\\src\\components\\Footer.js",[