Con esto el estado al recargar la pagina se vuelve a llenar y no queda vacio y puedo manipular un estado propio (loading) y puedo trabajar con el preloader.\r\n  const fetchData = async() => {\r\n    await getCity(id)\r\n    await getItineraries(id)\r\n    setLoading(false)\r\n  }\r\n\r\n  const comparator=()=>{\r\n    if(loading) {\r\n      return <Preloader/>\r\n    }if(props.allItineraries.length === 0 && !loading) {\r\n      return <NoItineraries/>\r\n    }\r\n  }\r\n  console.log(cities[0])\r\n  return (\r\n    <>\r\n    <CityHeader city={cities[0]}/>\r\n    <h3 style={{textAlign: \"center\"}}>Available Itineraries for {cities.cityName}</h3>\r\n    <div style={{display: 'flex', justifyContent: 'center'}}>{comparator()}</div>\r\n    {props.allItineraries.map(itinerary => {\r\n      return <Itinerary key={itinerary._id} itinerary={itinerary}/>\r\n    })}\r\n    <GoBack/>\r\n    </>\r\n  )\r\n}\r\n\r\nconst mapStateToProps = (state) => {\r\n  return {\r\n    cities: state.citiesR.cities,\r\n    allItineraries: state.itinerariesR.itineraries\r\n  }\r\n}\r\nconst mapDispatchToProps = {\r\n  getItineraries: itinerariesActions.getItineraries,\r\n  getCity: cityActions.getCity\r\n}\r\n\r\nexport default connect(mapStateToProps, mapDispatchToProps)(City)","D:\\Users\\Santiago\\Documents\\MindHub\\alessandro-mytinerary\\frontend\\src\\components\\CitiesHeader.js",[],"D:\\Users\\Santiago\\Documents\\MindHub\\alessandro-mytinerary\\frontend\\src\\components\\PathComparator.js",[],"D:\\Users\\Santiago\\Documents\\MindHub\\alessandro-mytinerary\\frontend\\src\\components\\SemiHero.js",[],"D:\\Users\\Santiago\\Documents\\MindHub\\alessandro-mytinerary\\frontend\\src\\components\\Preloader.js",[],"D:\\Users\\Santiago\\Documents\\MindHub\\alessandro-mytinerary\\frontend\\src\\components\\GoBack.js",[],"D:\\Users\\Santiago\\Documents\\MindHub\\alessandro-mytinerary\\frontend\\src\\components\\Images.js",[],"D:\\Users\\Santiago\\Documents\\MindHub\\alessandro-mytinerary\\frontend