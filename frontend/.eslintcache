\n    }\r\n  }\r\n\r\n\r\n  return(\r\n    <>\r\n    <SemiHero/>\r\n    <section className=\"cities\">\r\n      <input autoComplete=\"off\" className=\"input\" onChange={(e)=>props.citiesFilter(e.target.value)} type=\"text\" name=\"nombre\" \r\n        placeholder='Search Cities' style={{fontSize: '2.5vh'}} />\r\n      {comparator()}\r\n      <div className=\"cities\">\r\n        {props.searchCities.map((ciudad)=> { return <EachCity key={ciudad._id} ciudad={ciudad} /> })}\r\n      </div>\r\n    </section>\r\n    </>\r\n  )\r\n}\r\n\r\nconst mapStateToProps = state => {\r\n  return{\r\n    allCities: state.citiesR.cities,\r\n    searchCities: state.citiesR.searchCities,\r\n    userLogged: state.userR.userLogged\r\n  }\r\n}\r\nconst mapDispatchToProps = {\r\n  getCities: cityActions.getCities,\r\n  citiesFilter: cityActions.searchCities,\r\n  resetItineraries: itinerariesActions.resetItineraries\r\n}\r\n\r\nexport default connect(mapStateToProps, mapDispatchToProps)(Cities)","D:\\Users\\Santiago\\Documents\\MindHub\\alessandro-mytinerary\\frontend\\src\\components\\Hero.js",[],"D:\\Users\\Santiago\\Documents\\MindHub\\alessandro-mytinerary\\frontend\\src\\components\\CityHeader.js",[],"D:\\Users\\Santiago\\Documents\\MindHub\\alessandro-mytinerary\\frontend\\src\\components\\City.js",[],"D:\\Users\\Santiago\\Documents\\MindHub\\alessandro-mytinerary\\frontend\\src\\components\\CitiesHeader.js",[],"D:\\Users\\Santiago\\Documents\\MindHub\\alessandro-mytinerary\\frontend\\src\\components\\PathComparator.js",[],"D:\\Users\\Santiago\\Documents\\MindHub\\alessandro-mytinerary\\frontend\\src\\components\\SemiHero.js",[],"D:\\Users\\Santiago\\Documents\\MindHub\\alessandro-mytinerary\\frontend\\src\\components\\Preloader.js",[],"D:\\Users\\Santiago\\Documents\\MindHub\\alessandro-mytinerary\\frontend\\src\\components\\GoBack.js",[],"D:\\Users\\Santiago\\Documents\\MindHub\\alessandro-mytinerary\\frontend\\src\\components\\Images.js",[],"D:\\Users\\Santiago\\Documents\\MindHu